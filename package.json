{
  "name": "@noctuatech/pdf-cleaner",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "pkg"
  ],
  "main": "pkg/pdf_cleaner.js",
  "module": "pkg/pdf_cleaner.js",
  "exports": {
    ".": {
      "import": "./pkg/pdf_cleaner.js"
    }
  },
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "./scripts/build.sh",
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "scripts/build.sh"
      ],
      "output": [
        "target/**",
        "pkg/**"
      ],
      "dependencies": []
    },
    "test": {
      "command": "cargo test"
    }
  },
  "dependencies": {}
}
